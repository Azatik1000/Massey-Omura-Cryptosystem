#!/home/azat/Projects/Programming/algebra_practice/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pyfinite==1.7','console_scripts','sample'
__requires__ = 'pyfinite==1.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyfinite==1.7', 'console_scripts', 'sample')()
    )
